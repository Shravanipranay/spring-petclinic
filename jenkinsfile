node('UBUNTU_NODE1'){
    stage('vcs'){
        git branch: 'scripted', url: 'https://github.com/Shravanipranay/spring-petclinic.git'
    }
    stage('build') {
        sh 'export PATH=$PATH:/opt/mvn/bin'
        sh 'mvn package'
    }
    stage('archiving artifacts'){
        archiveArtifacts artifacts : '**/spring-petclinic-3.0.0-SNAPSHOT.jar'
        junit: '**/*.xml'
    }
}
